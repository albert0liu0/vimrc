syntax on
set showcmd
set relativenumber
set number
set cindent
set mouse=a
set cmdheight=2
set expandtab
set shiftwidth=4
set softtabstop=4
colorscheme elflord
set t_Co=256
highlight LineNr ctermfg=grey
highlight CursorLineNr ctermfg=black ctermbg=grey
set cursorline
set cursorcolumn
highlight CursorLine ctermbg=232 cterm=NONE
highlight CursorColumn ctermbg=232 cterm=NONE
highlight Search ctermbg=NONE cterm=underline ctermfg=NONE
set hlsearch
